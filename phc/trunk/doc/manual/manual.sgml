<set id="manual">
<title>&phc Documentation</title>
<book id="usermanual">
	<title>The &phc User's Manual</title>
	&install
	&runningphc
</book>
<book id="devmanual">
	<title>The &phc Developer's Manual</title>
	<part id="treetutorials">
		<title>Tree Traversal API Tutorials</title>
		&gettingstarted
		&treetutorial1
	</part>
	<part id="reference">
		<title>Reference</title>
		&grammar
		&representingphp
		&maketeatheory
	</part>
</book>
</set>
